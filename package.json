{
	"name": "AppZombieProjectName",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"start": "react-native start",
		"start:android": "npm-run-all --parallel android:reverse start",
		"log:android": "adb logcat *:S ReactNative:V",
		"android:reverse": "adb reverse tcp:8081 tcp:8081 && adb reverse tcp:7007 tcp:7007 && adb reverse tcp:51349 tcp:51349",
